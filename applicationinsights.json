{
  "role": {
    "name": "crime-portal-gateway",
    "instance": "crime-portal-gateway"
  },
  "instrumentation": {
    "logging": {
      "level": "DEBUG"
    }
  },
  "sampling": {
    "overrides": [
      {
        "attributes": [
          {
            "key": "http.url",
            "value": "https?://[^/]+/health.*",
            "matchType": "regexp"
          }
        ],
        "percentage": 100
      }
    ]
  },
  "heartbeat": {
    "intervalSeconds": 15
  }
}
