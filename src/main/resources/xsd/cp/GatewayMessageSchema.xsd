	<xs:element name="GatewayOperationType">
		<xs:complexType>
			<xs:choice>

				<!--ACKNOWLEDGEMENT-->
				<xs:element ref="cp-edrreq:Acknowledgement"/>

				<!-- EXTERNAL DOCUMENT -->
				<xs:element ref="cp-edrreq:ExternalDocumentRequest"/>

			</xs:choice>
